{
  "name": "@fnx/ui",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --dts --external react --minify",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf node_modules dist .turbo && echo 'UI Cleanup complete!'"
  },
  "devDependencies": {
    "@fnx/eslint-config": "workspace:*",
    "@fnx/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.31.0",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
